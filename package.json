    "release:trigger": "shipjs trigger",
    "prepare": "husky install"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./configs/jest.setup.ts"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/src/**/*.spec.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
